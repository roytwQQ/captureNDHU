<!-- The #page-top ID is part of the scrolling feature - the data-spy and data-target are part of the built-in Bootstrap scrollspy function -->

<body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">

    <div id="fb-root"></div>

    

    <!-- Navigation -->
    <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand page-scroll" href="#page-top">Start Bootstrap</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav">
                    <!-- Hidden li included to remove active class from about link when scrolled up past about section -->
                    <li class="hidden active">
                        <a class="" href=""></a>
                    </li>
                    <li class="">
                        <a class="" href="/photos/upload">貢獻獵物</a>
                    </li>
                    <li class="">
                        <a class="" href="/photos">欣賞圖鑑</a>
                        <!-- 做成遊戲依樣 有卡片編號 -->
                    </li>
                    <li class="">
                        <a class="" href="/photos/ranking">排名</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-- Intro Section -->
    <section id="" class=""  >
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                   




                    <div id="photos" class="col-lg-12" style="margin-top:100px;">
                        <div class="row">
                    <% photos.forEach(function(photo, index) { %>

	                    <div class=" col-sm-4" style="margin-bottom:50px;">
		                    <!-- <div><%=photo.name%></div> -->
                            <!-- <div><%=photo._id%></div> -->
                            <div   class="fb-like" data-href="http://young-forest-9275.herokuapp.com/photos/<%=photo._id%>" data-width="100px" data-layout="standard" data-action="like" data-show-faces="false" data-share="false"></div>
                            <div style="margin:5px 0;">

                                
                                <a id="map_<%=photo._id%>" data-href="http://192.168.1.4:3000/photos/<%=photo.path%>" data-latitude="<%=photo.latitude%>" data-longitude="<%=photo.longitude%>" class="btn btn-default various fancybox.iframe picmap" href="/picmap.htm">map</a>
                            
                            
                            <a class="btn btn-default various" href="#hideInfo_<%=index %>">more detail</a>
                            

                            </div>
                            
                            <div>
                                <a class="fancybox" href="/photos/<%=photo.path%>">
		                    <img  style="width:90%;" class="photos" src='/photos/<%=photo.path%>'/>
                                </a>
                            </div>
                            <div id="hideInfo_<%=index %>" style="display:none;">
                                <table class="table table-bordered table-striped">
                                    <tr>
                                        <td>Owner</td>
                                        <td><%=photo.owner%></td>
                                    </tr>
                                    <tr>
                                        <td>名稱</td>
                                        <td><%=photo.title%></td>
                                    </tr>
                                    <tr>
                                        <td>日期</td>
                                        <td><%=photo.date%></td>
                                    </tr>
                                    <tr>
                                        <td>秘訣</td>
                                        <td><%=photo.camera%></td>
                                    </tr>
                                    <tr>
                                        <td>雜記</td>
                                        <td><%=photo.comment%></td>
                                    </tr>
                                    
                                </table>
                                
                                
                              
                    


                            

                              
                               	
                               </div>
                            </div>
	                    
                    <% }) %>
                </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    


