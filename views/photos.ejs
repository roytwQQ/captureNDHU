        <script>
            $(document).ready(function() {
                console.log($(this).attr('data-href'));
                $(".picmap").click(function() {
                    if($(this).attr('data-latitude') == "" || $(this).attr('data-longitude') == ""){
                        alert("這張照片沒有位置資訊喔><");
                        return false;
                    }
                    // console.log($(this).attr('data-latitude'));
                    // console.log($(this).attr('data-longitude'));
                    localStorage.temp_latitude = $(this).attr('data-latitude');
                    localStorage.temp_longitude = $(this).attr('data-longitude');
                    localStorage.temp_url = $(this).attr('data-href');


             });
